modules = ["python-3.11"]

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 38859788
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install -r requirements.txt && cd n8n-voice-interface && python3 replit-main.py"

[nix]
channel = "stable-24_05"

[[ports]]
localPort = 8080
externalPort = 80

[deployment]
run = ["sh", "-c", "python replit-main.py"]
